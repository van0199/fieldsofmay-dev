
/* Global CSS----------------------------------------------------------------------------------------------------------------- */

/* hide scrollbar but allow scrolling */
div {
  /* -ms-overflow-style: none;  */
  /* for Internet Explorer, Edge */
  /* scrollbar-width: none;  */
  /* for Firefox */
  /* overflow-y: scroll;  */
}

div::-webkit-scrollbar {
  display: none; /* for Chrome, Safari, and Opera */
}

:root {
  --s-5: 5px;
  --s-10: 10px;
  --s-20: 20px;
  --s-40: 40px;
  --s-80: 80px;
  --s-160: 160px;
  --s-320: 320px;
  --m-40: -40px;
  --m-20: -20px;
  --s-content: calc(100vh - var(--s-80) - var(--s-10));
  /* --s-content-ex-small: calc(1px * var(--full-width)); */
  --s-content-ex-small: .9rem;
  --full-width: 100vw;
  --containerHeader: 'IBMPlexMono';
  --bot-spaceing: .1rem;
  --xx-small-font: clamp(.75rem,.10vw,.1rem);
  --exx-small-font: clamp(.75rem,10vw,.9rem);
  --ex-small-font: clamp(.75rem,10vw,1rem);
  --small-font: 14pt;
  --med-font: 24pt;
  --large-font: 24.425vw;
  --large-font: 18.425vw;
  --xl-large-font: 20.425vw;
  --line-height: 16.425vw;
}


/* Hide scrollbar for Chrome, Safari and Opera */
body::-webkit-scrollbar {
  display: none;
}

/* Hide scrollbar for IE, Edge and Firefox */
body {
  -ms-overflow-style: none;  /* IE and Edge */
  scrollbar-width: none;  /* Firefox */
}



