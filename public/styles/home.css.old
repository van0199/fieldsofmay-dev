html {
    font-size: 100%;
    box-sizing: border-box;
    
}

* , * ::before, * ::after {
box-sizing: inherit;

}


html,
body {
  font-family: Arial, Helvetica, sans-serif;
  line-height: 1.2;
  padding: 0;
  margin: 0;
  min-height: 100vh;
  /* color: hsl(0, 0%, 6%); */
  color: #000dff;
  overflow-y: hidden;
  overflow-x: hidden;
  cursor: url("cur.png"), url("cur.cur"), default;
  cursor: url(cur.png), auto;
  /* font-family: 'IBMPlexMono';  */
    background: #00002C;
    transition: all 0.75s ease-in-out;
    transition: opacity 0.35s, padding-left 0.75s;
    animation: fadein 2s;
    -moz-animation: fadein 2s; /* Firefox */
    -webkit-animation: fadein 2s; /* Safari and Chrome */
    -o-animation: fadein 2s; /* Opera */
    background: radial-gradient(circle at center , #535353bf 0,#271d7cde 20%,#00002c, #050567 100%);
}

a{
  cursor: url("cur.png"), url("cur.cur"), default;
  cursor: url(cur.png), auto;
}


h1 {
	margin-top: 0;
	font-size: 2rem;
	line-height: 1;
}

a {
  color: inherit;
  text-decoration: none;
}


section{
  transition: margin-top 0.75s ease-in-out;
  min-height: calc(2vh - .25vw);
  width: 99vw;
  transition: all 0.75s ease-in-out;
}

* {
  box-sizing: border-box;
}

  @keyframes fadein {
    from {
        opacity:0;
    }
    to {
        opacity:1;
    }
}
@-moz-keyframes fadein { /* Firefox */
    from {
        opacity:0;
    }
    to {
        opacity:1;
    }
}
@-webkit-keyframes fadein { /* Safari and Chrome */
    from {
        opacity:0;
    }
    to {
        opacity:1;
    }
}
@-o-keyframes fadein { /* Opera */
    from {
        opacity:0;
    }
    to {
        opacity: 1;
    }
}



select:focus {
  background-position:
    calc(100% - 15px) 1em,
    calc(100% - 20px) 1em,
    calc(100% - .5em) .5em;
  background-size:
    5px 5px,
    5px 5px,
    1.5em 1.5em;
  background-repeat: no-repeat;
  border-color: rgba(0, 128, 0, 0);
  outline: 0;
}

select:-moz-focusring {
  color: transparent;
  text-shadow: 0 0 0 #000;
}


